/* pages/mine/mine.wxss */
.mine-header {
    padding: 20px 0px 30px;
    background-color: #fff;
    margin-bottom: 5px;
    display: flex;
}
.mine-header-logo {
    width: 50px;
    height: 50px;
    margin-left: 20px;
    border-radius: 5px;
    overflow: hidden;
}
.mine-header-right {
    display: flex;
    flex-direction: column;
    flex: 1;
    padding: 0px 10px 0px 20px;
}
.mine-name {
    /* font-weight: bold; */
    margin-bottom: 5px;
}
.mine-sub {
    position: relative;
    color: #999;
    font-size: 14px;
}
.postion-div {
    position: absolute;
    right: 0;
    top: 0;
    bottom: 0;
    margin-top: auto;
    margin-bottom: auto;
    padding-right: 5px;
}
.mine-icon {
    margin-left: 10px;
}
.van-cell-group {
    margin-bottom: 5px;
}
.van-cell__title {
    padding-left: 5px;
}